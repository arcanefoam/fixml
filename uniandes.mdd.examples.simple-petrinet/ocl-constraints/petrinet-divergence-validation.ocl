import '../metamodels/petrinet.ecore'

package PN
	
context PetriNet inv :
	not elements->exists(t| t.oclIsTypeOf(Transition) 
		and elements->select(i|i.oclIsTypeOf(InputArc) 
		and i.oclAsType(InputArc).to = t)->size() < 
				elements->select(o|o.oclIsTypeOf(OutputArc) 
		and o.oclAsType(OutputArc).from = t)->size())

endpackage